<letter> ::= [a-zA-Z]+ ;
<number> ::= [0-9]+;
<character> ::= letter | number | symbol | "_";
<annotationName> ::= "//@", ["getSequenceConstructor" | "getSequenceMethod"];
<attrName>  ::= "name", "=",  character, {character | number| "_"}+ ;
<attrArgs> ::= "args", "=", [{character | "_"}+ | "none"];
<attrSequence> ::= "sequence", "=", number ;
<attrSequencebyconstructor> ::="sequencebyconstructor", "=", number;


<jGetSequenceConstructor> ::= annotationName, attrName, ",", attrArgs, ",", attrSequence, ";";
<jGetSequenceMethod> ::= annotationName, attrName, ",", attrArgs, ",", attrSequence, ",", attrSequencebyconstructor, ";";

<rule> ::= jGetSequenceConstructor | jGetSequenceMethod;
<grammar> ::= { rule } ;

